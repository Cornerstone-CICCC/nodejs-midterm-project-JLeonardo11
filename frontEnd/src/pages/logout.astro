<script>
    const logout = async () => {
      const res = await fetch(`http://localhost:3600/users/logout`, {
        method: "GET",
        credentials: "include"
      })
      if (!res.ok) {
        console.error("Something went wrong")
        return
      }
      window.location.href = "/login"
    }
  
    logout()
  </script>